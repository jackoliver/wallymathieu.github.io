---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Your Website</title>
  <link href="http://example.com/"/>
  <link type="application/atom+xml" rel="self" href="http://example.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://assertfail.gewalli.se/</id>
  <author>
    <name>Oskar Gewalli</name>
    <email>webmaster@gewalli.se</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://assertfail.gewalli.se{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://assertfail.gewalli.se{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
    <name>Oskar Gewalli</name>
      <uri>http://gewalli.se/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
